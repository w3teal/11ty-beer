<!doctype html>
<html lang="en">

<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta name="description" content="{{ description }}">
  <meta name="author" content="{{ site.author }}">
  <meta name="generator" content="Eleventy v3.0.0">
  <meta name="theme-color" content="#ffffff" />
  <link rel="icon" type="image/png" sizes="32x32" href="/favicon/ios/32.png">
  <link rel="apple-touch-icon" href="/favicon/ios/64.png">
  <title>{{ title }}</title>
  <link rel="stylesheet" href="/modules/gmx.css">
  <link rel="stylesheet" href="/modules/theme.css">
  <script src="/modules/iconify-icon.min.js"></script>
  <link rel="manifest" href="/manifest.json">
  <style>
    .sr-only {
      position: absolute;
      width: 1px;
      height: 1px;
      padding: 0;
      margin: -1px;
      overflow: hidden;
      clip: rect(0, 0, 0, 0);
      white-space: nowrap;
      border-width: 0;
    }

    *:has(>.header-anchor) {
      position: relative;
    }

    *:has(>.header-anchor):hover .header-anchor {
      opacity: 1;
      visibility: visible;
    }

    .header-anchor {
      opacity: 0;
      transition: .1s;
      position: absolute;
      left: 0;
      margin-left: -.75em;
      background-position: 0 0%;
      visibility: hidden;
    }

    .header-anchor::before {
      content: "#";
    }
  </style>
</head>

<body class="{{ site.theme }}">
  {{ content | safe }}
</body>

{% include "components/register-sw.njk" %}

</html>